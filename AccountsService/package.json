{
  "name": "accountsservice",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node build/src/main.js",
    "dev": "npm run init:dev & nodemon -q -w ./build/src ./build/src/main.js",
    "prod": "npm run init:prod && npm start",
    "init:prod": "npm-run-all build:clean build:release",
    "init:dev": "npm-run-all build:clean build:watch",
    "build:clean": "rimraf coverage build tmp",
    "build:release": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.7",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  }
}
